#1
print("True and True:", True and True, "\n") # True
#2
print("False and True:", False and True, "\n") # False
#3
print("1 == 1 and 2 == 1:", 1 == 1 and 2 == 1, "\n") # False
#4
print('"test" == "test":', "test" == "test", "\n") # True
#5
print("1 == 1 or 2 != 1:", 1 == 1 or 2 != 1, "\n") # True
#6
print("True and 1 != 1:", True and 1 != 1, "\n") # False
#7
print("False and 0 != 0:", False and 0 != 0, "\n") # False
#8
print("True or 1 == 1:", True or 1 == 1, "\n") # True
#9
print('"test" == "testing":', "test" == "testing", "\n") # False
#10
print("1 != 0 and 2 == 1:", 1 != 0 and 2 == 1, "\n") # False
#11
print('"test" != "testing":', "test" != "testing", "\n") # True
#12
print('"test" == 1:', "test" == 1, "\n") # False
#13
print("not (True and False):", not (True and False), "\n") # True
#14
print("not (1 == 1 and 0 != 1):", not (1 == 1 and 0 != 1), "\n") # False
#15
print("not (10 == 1 or 1000 == 1000):", not (10 == 1 or 1000 == 1000), "\n") # False
#16
print("not (1 != 10 or 3 == 4):", not (1 != 10 or 3 == 4), "\n") # False
#17
print('not ("testing" == "testing" and "Zed" == "Cool Guy"):', not ("testing" == "testing" and "Zed" == "Cool Guy"), "\n") # True
#18
print("1 == 1 and (not ('testing' == 1 or 1 == 0)):", 1 == 1 and (not ("testing" == 1 or 1 == 0))) # True
#19
print("'chunky' == 'bacon' and (not (3 == 4 or 3 == 3)):", "chunky" == "bacon" and (not (3 == 4 or 3 == 3))) # False
#20
print("3 == 3 and (not ('testing' == 'testing' or 'Python' == 'Fun')):", 3 == 3 and (not ("testing" == "testing" or "Python" == "Fun")) # False 
